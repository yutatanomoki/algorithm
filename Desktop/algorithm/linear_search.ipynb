    "def linear_search(data, value):\n",
    "    for i in range(len(data)):\n",
    "        if data[i] == value:\n",
    "            return i\n",
    "    return -1\n",
    "\n",
    "data = [50, 30, 90, 10, 20, 70, 60, 40, 80]\n",
    "print(linear_search(data, 40))"
   ]
